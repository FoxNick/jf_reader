{
    "www.xbiquge.cc" : {
        "rootURL":"https://www.xbiquge.cc/",
        "search":{
            "url":"https://www.xbiquge.cc/modules/article/search.php?searchkey=#bookname#",
            "encodeType":"gbk",
            "decodeType":"gbk",
            "resultElements":"#main .novelslistss li",
            "elementParse":{
                "name": {
                    "selector": ".s2 a",
                    "type": "inner"
                },
                "latest": {
                    "selector":".s3 a",
                    "type": "inner"
                },
                "author": {
                    "selector":".s4",
                    "type": "inner"
                },
                "catalogUrl": {
                    "selector":".s2 a",
                    "type": "attr",
                    "attrKey": "href"
                }
            },
            "notFound":{
                "resultElements":"body",
                "elementParse":{
                    "name": {
                        "selector": "#info h1",
                        "type": "inner"
                    },
                    "latest": {
                        "selector":"#info p:nth-last-of-type(1) a",
                        "type": "inner"
                    },
                    "author": {
                        "selector":"#info p a",
                        "type": "inner"
                    },
                    "catalogUrl": "#currentUrl#"
                }
            },
            "errorMsg":"请30秒后再试"
        },
        "catalog":{
            "chapterElements":"#list dd",
            "chapterName":"a",
            "chapterUrl":"a"
        },
        "chapter":{
            "contentElement":"#content"
        }
    }
}